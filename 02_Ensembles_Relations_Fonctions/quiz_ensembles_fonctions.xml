  <question type="multichoice">
    <name><text>Injective / Surjective / Bijective</text></name>
    <questiontext format="html">
      <text><![CDATA[Quel énoncé caractérise l’injectivité de f : E→F ?]]></text>
    </questiontext>
    <generalfeedback><text></text></generalfeedback>
    <defaultgrade>1.000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text><![CDATA[$f(x)=f(y) \Rightarrow x=y$]]></text>
      <feedback><text><![CDATA[Définition usuelle de l'injectivité.]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$\forall y\in F,\, \exists x\in E:\ f(x)=y$]]></text>
      <feedback><text><![CDATA[C'est la surjectivité.]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$\exists g:F\to E:\ g\circ f=\mathrm{id}_E$]]></text>
      <feedback><text><![CDATA[Cela implique l'injectivité mais ce n'est pas la définition la plus directe.]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$\exists g:F\to E:\ f\circ g=\mathrm{id}_F$]]></text>
      <feedback><text><![CDATA[Cela caractérise la surjectivité.]]></text></feedback>
    </answer>
  </question>
