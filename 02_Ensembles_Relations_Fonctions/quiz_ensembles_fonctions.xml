  <question type="multichoice">
    <name><text>Sous-ensemble (définition)</text></name>
    <questiontext format="html">
      <text><![CDATA[Soient A et B des ensembles. Quelle écriture formalise correctement « A est un sous-ensemble de B » ?]]></text>
    </questiontext>
    <generalfeedback><text></text></generalfeedback>
    <defaultgrade>1.000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text><![CDATA[$$A \subseteq B \iff \forall x\,(x\in A \Rightarrow x\in B).$$]]></text>
      <feedback><text><![CDATA[Définition standard de l'inclusion.]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$A \subset B \iff \forall x\,(x\in B \Rightarrow x\in A).$$]]></text>
      <feedback><text><![CDATA[]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$A \subseteq B \iff \exists x\,(x\in A \Rightarrow x\in B).$$]]></text>
      <feedback><text><![CDATA[]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$A \subseteq B \iff \Card(A)\le \Card(B).$$]]></text>
      <feedback><text><![CDATA[L'inclusion n'est pas une simple comparaison de cardinaux.]]></text></feedback>
    </answer>
  </question>
  <question type="multichoice">
    <name><text>Lois de De Morgan</text></name>
    <questiontext format="html">
      <text><![CDATA[Dans un univers U fixé, quelle égalité est correcte ?]]></text>
    </questiontext>
    <generalfeedback><text></text></generalfeedback>
    <defaultgrade>1.000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <answer fraction="100">
      <text><![CDATA[$$(A\cup B)^c = A^c \cap B^c$$]]></text>
      <feedback><text><![CDATA[Première loi de De Morgan.]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$(A\cap B)^c = A^c \cap B^c$$]]></text>
      <feedback><text><![CDATA[]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$(A\cup B)^c = A^c \cup B^c$$]]></text>
      <feedback><text><![CDATA[]]></text></feedback>
    </answer>
    <answer fraction="0">
      <text><![CDATA[$$(A\cap B)^c = A \cup B$$]]></text>
      <feedback><text><![CDATA[]]></text></feedback>
    </answer>
  </question>
